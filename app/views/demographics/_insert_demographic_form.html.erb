<%= form_for (Demographic.new) do |f| %>
  <%= f.hidden_field :state, :value => @demographic["state"] %>
  <%= f.hidden_field :city, :value => @demographic["city"] %>
  <%= f.hidden_field :household_income, :value => @demographic["medianHouseholdIncome"] %>
  <%= f.hidden_field :single_family_home, :value => @demographic["medianSingleFamilyHome"] %>
  <%= f.hidden_field :single_males, :value => @demographic["singleMales"] %>
  <%= f.hidden_field :single_females, :value => @demographic["singleFemales"] %>
  <%= f.hidden_field :median_age, :value => @demographic["medianAge"] %>
  <%= f.hidden_field :homes_with_kids, :value => @demographic["homesWithKids"] %>
  <%= f.hidden_field :owners, :value => @demographic["owners"] %>
  <%= f.hidden_field :renters, :value => @demographic["renters"] %>
  <%= f.hidden_field :latitude, :value => @demographic["latitude"] %>
  <%= f.hidden_field :longitude, :value => @demographic["longitude"] %>
  <%= f.submit %>

<% end %>
